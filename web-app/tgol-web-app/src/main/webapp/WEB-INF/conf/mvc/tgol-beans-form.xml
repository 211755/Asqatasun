        <property name="value" value="NEED_MORE_INFO"/>
        <property name="i18nKey" value="nmi"/>
        <property name="errorI18nKey" value="nmi-error"/>
        <property name="default" value="false"/>
        <property name="enabled" value="true"/>
    </bean>

    <bean id="naSelectElementBuilder" class="org.opens.tgol.form.builder.SelectElementBuilderImpl">
        <property name="value" value="NOT_APPLICABLE"/>
        <property name="i18nKey" value="na"/>
        <property name="errorI18nKey" value="na-error"/>
        <property name="default" value="false"/>
        <property name="enabled" value="true"/>
    </bean>

    <bean id="testResultSelectThemeSelectFormFieldBuilder" class="org.opens.tgol.form.builder.SelectFormFieldBuilderImpl">
        <property name="i18nKey" value="result.test-result"/>
        <property name="errorI18nKey" value="result.test-result-error"/>
        <property name="selectElementBuilderMap">
            <map>
                <entry key="test-result">
                    <list>
                        <ref local="allResultSelectElementBuilder"/>
                        <ref local="passedSelectElementBuilder"/>
                        <ref local="failedSelectElementBuilder"/>
                        <ref local="nmiSelectElementBuilder"/>
                        <ref local="naSelectElementBuilder"/>
                    </list>
                </entry>
            </map>
        </property>
    </bean>

</beans>