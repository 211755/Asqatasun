<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <bean id="parameterElementDataService" class="org.opens.tanaguru.entity.service.parameterization.MockParameterElementDataService"/>
    <bean id="parameterDataService" class="org.opens.tanaguru.entity.service.parameterization.MockParameterDataService"/>

    <bean id="userDataService" class="org.opens.tgol.entity.service.user.MockUserDataService"/>

    <bean id="contractDataService" class="org.opens.tgol.entity.service.contract.MockContractDataService">
        <property name="userDataService" ref="userDataService"/>
    </bean>

</beans>